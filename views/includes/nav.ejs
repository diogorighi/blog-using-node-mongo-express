<nav id="mainNav" class="navbar navbar-default navbar-fixed-top">
	<div class="container-fluid">
	    <!-- Brand and toggle get grouped for better mobile display -->
	    <div class="navbar-header">
	        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
	            <span class="sr-only">Toggle navigation </span>
	            <span class="icon-bar"></span>
	            <span class="icon-bar"></span>
	            <span class="icon-bar"></span>
	        </button>

	        <%  if ( 
	        		( page.indexOf("/blog") >= 0 ) || ( page.indexOf("/post") >= 0 ) 
	        	) { 
	        	var linkTitle = settings.url;
	        	var linkMenu = settings.url + '/#';
	        } else { 
	        	var linkTitle = "#page-top";
	        	var linkMenu = '#';
	        } %>
	        <a class="navbar-brand page-scroll" href="<%= linkTitle %>"> <%= settings.title %> </a>
	    </div>

	    <!-- Collect the nav links, forms, and other content for toggling -->
	    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
	        <ul class="nav navbar-nav navbar-right">

	        	<!-- Creating menu -->
	        	<% 	
				var menuItems = [
						{ title: 'Blog', goTo :  settings.url + '/blog' },
						{ title: 'Sobre', goTo : linkMenu + 'about' },
						{ title: 'Serviços', goTo : linkMenu + 'services' },
						{ title: 'Portfólio', goTo : linkMenu + 'portfolio' },
						{ title: 'Contato', goTo : linkMenu + 'contact' }
				  	];
	        	menuItems.forEach( function(menu) { 
	        	%>
		        	<li>
		                <a class="page-scroll" href="<%=menu.goTo%>"> <%=menu.title%> </a>
		            </li>
	        	<% }); %>
	        	
	        </ul>
	    </div>
	    <!-- /.navbar-collapse -->
	</div>
	<!-- /.container-fluid -->
</nav>